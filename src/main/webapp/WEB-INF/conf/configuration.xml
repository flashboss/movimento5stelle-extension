  <import>war:/conf/termsandconditions/web-filter-configuration.xml</import>
  
  <!-- Import dell'acme intranet-->
  <import>war:/conf/platform/portal/resource-bundle-configuration.xml</import>
  <import>war:/conf/office-extension/organization/organization-configuration.xml</import>
  <import profiles="social,all,default">war:/conf/office-extension/social/spaces-configuration.xml</import>
  <import>war:/conf/office-extension/wcm/deployment/office-deployment-configuration.xml</import>
