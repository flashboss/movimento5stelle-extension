  <!-- builtin configuration -->
  <import>war:/conf/platform/jcr-configuration.xml</import>
  <import>war:/conf/platform/dms/dms-configuration.xml</import>
  <import>war:/conf/platform/dms/dms-ext-configuration.xml</import>
  <import>war:/conf/platform/chat/chat-configuration.xml</import>

  <!-- IDE -->
  <!-- import>war:/conf/platform/ide-jcr-configuration.xml</import>
  <import>war:/conf/platform/ide-jcr-component-plugins-configuration.xml</import>
  <import>war:/conf/platform/ide-configuration.xml</import -->
  
  <!-- portal templates configuration-->
  <import>war:/conf/platform/portal/portal-configuration.xml</import>
  <!-- Add Favicon filter-->
  <import>war:/conf/platform/portal/web-filter-configuration.xml</import>
  <!-- Configure User Dashboard  -->
  <!--  <import>war:/conf/platform/portal/users-dashboard-configuration.xml</import>-->
  <!-- Configure the profile contact provider -->
  <import>war:/conf/platform/ks/default-profile-configuration.xml</import>
  <import profiles="social,default,all">war:/conf/platform/ks/profile-configuration.xml</import>

  <!-- Add portal templates artifacts -->
  <import>war:/conf/platform/wcm/deployment/template-empty-deployment-configuration.xml</import>

  <!-- Redefine TransactionService timeout-->
