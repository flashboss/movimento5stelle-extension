  <import>war:/conf/office-extension/wcm/taxonomy/intranet-taxonomies-configuration.xml</import>
  <import>war:/conf/office-extension/dms/dms-views-configuration.xml</import>
  <import>war:/conf/office-extension/common/common-configuration.xml</import>
  <import>war:/conf/office-extension/wcm/deployment/templates/template-intranet-deployment-configuration.xml</import>
  <import>war:/conf/office-extension/ks/ks-configuration.xml</import>

</configuration>
